# MCP Client Configuration
# Copy this file to .env and fill in your values

# OpenRouter API Key (Required for LLM features)
OPENROUTER_API_KEY=your-openrouter-api-key-here

# Default model to use
DEFAULT_MODEL=z-ai/glm-4.5-air:free

# Session secret for production (generate a random string)
SESSION_SECRET=your-secure-random-session-secret-here

# CORS allowed origins (comma-separated)
ALLOWED_ORIGINS=http://localhost:8080,https://yourdomain.com

# Notion OAuth Configuration (Optional - for Notion MCP integration)
NOTION_CLIENT_ID=your-notion-client-id
NOTION_CLIENT_SECRET=your-notion-client-secret
NOTION_REDIRECT_URI=http://localhost:8000/api/notion/callback

# Debug settings
DEBUG=false
LOG_LEVEL=INFO
